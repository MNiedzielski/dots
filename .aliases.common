        alias   forall  "find . -name '[a-z]*' -maxdepth 1 -type d -exec sh -c 'echo "\$0" && cd "\$0" && \!*' {} \;"
        alias   gab     "find . -name '[a-z]*' -maxdepth 1 -type d -exec sh -c 'echo "\$0" && git -C "\$0" branch' {} \;"
