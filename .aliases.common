	alias	c	'set d=`echo \!^* | sed "s/./\/&*/g"`; set d = `dirp $d`; cd $d || echo $d'
	alias	cd	'set tmp=$cwd; chdir \!*; set old=$tmp; sethdr'
	alias	cgrep	"grep \!* *.[Cch]"
	alias	clean	"\rm -f #* *~ .*~ *%"
        alias   nclone  'for i in aws-account aws-acra aws-lock aws-noc aws-supportbridge aws-updates devops-docs devops-tools puppet-acra puppet-base puppet-noc puppet-supportbridge puppet-updates python-lock python-noc retired-nasuni retired-rackspace static-dashboard; do git clone ssh://git-codecommit.us-east-1.amazonaws.com/v1/repos/$i; done'
	alias	clpr	'vgrind \!* -t | lpr -t -h'
