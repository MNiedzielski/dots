(custom-set-variables
 '(ecb-options-version "2.24")
 '(column-number-mode t)
 '(toolbar-visible-p nil)
 '(default-gutter-position (quote left))
 '(line-number-mode t)
 '(ecb-layout-window-sizes (quote (("left8" (0.1928571428571429 . 0.2857142857142857) (0.1928571428571429 . 0.25) (0.1928571428571429 . 0.2857142857142857) (0.1928571428571429 . 0.1785714285714286)))))
 '(ecb-source-path (quote (("c:" "c:") ("y:" "y:") ("z:" "z:"))))
; '(blink-cursor-mode t nil (blink-cursor)))
(custom-set-faces
 '(default ((t (:size "8pt" :family "Courier New"))) t)
 '(paren-mismatch ((t (:background "" :strikethru t))) t)
 '(custom-changed-face ((((class color)) (:foreground "white" :background "blue" :italic t))))
 '(font-lock-doc-string-face ((((class color) (background light)) (:foreground "green4"))))
 '(paren-match ((t (:background "darkseagreen2"))) t))

(setq minibuffer-max-depth nil)
